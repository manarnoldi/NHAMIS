
@{
    ViewBag.Title = "Medals";
}

<div class="form-row">
    <div class="form-group description-header">
        <br />
        <h3><u> Add Medals</u></h3>
    </div>

</div>

<form>

    <div class="form-row">
        <div class="form-group col-md-6">
            <label for="Name">Name </label>
            <input type="text" class="form-control" id="Name" placeholder=" Names">
        </div>
        <div class="form-group col-md-6">
            <label for="Abbreviation">Abbreviation </label>
            <input type="text" class="form-control" id="Abbreviation" placeholder="Abbreviation">
        </div>

    </div>


    <div class="form-row">
        <div class="form-group col-md-6">
            <label for="Quantity">Quantity </label>
            <input type="number" class="form-control" id="Quantity" placeholder=" Quantity">
        </div>
        <div class="form-group col-md-12">

        </div>

    </div>

    <div class="form-row">
        <div class="form-group col-md-6">

            <button type="submit" class="btn btn-default"> Save</button>

        </div>
        <div class="form-group col-md-6">

            <button type="submit" class="btn btn-danger">Clear</button>

        </div>
    </div>

</form>

